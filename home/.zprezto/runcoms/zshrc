# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...
if [[ -s "${ZDOTDIR:-$HOME}/.zsh.d" ]]; then
  for i in `~/.zsh.d/init.sh`; do source ~/.zsh.d/$i; done
fi

USERNAME='Alexandre M. S.'
TODAY=`date +%d/%m/%Y`
